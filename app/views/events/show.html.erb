<h1>show event page</h1>



<p>
	<strong>Location:</strong> <%= @event.location %><br/> 
	<strong>Event Date:</strong> <%= @event.event_date %><br/> 
	<strong>Creator:</strong> <%= @user.username %>		
</p>

<% @event.attendees.each do |attendee| %>
	<h4>Attendees: </h4>
		<%= attendee.username %><br />
		<%= attendee.email %>
<% end %>